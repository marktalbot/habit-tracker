<h4>New Calendar</h4>
<div class="form">
   <form action="">
        <p><label for="name">Name:</label></p>
        <p>{{ input value=model.name type="text" id="name" }}</p>
        <p><label for="name">Notes:</label></p>
        <p>{{ textarea value=model.notes id="notes" }}</p>
        <p>
            <button {{ action "save" }}>Save</button>
            <button {{ action "cancel" }}>Cancel</button>
        </p>
   </form>
</div>